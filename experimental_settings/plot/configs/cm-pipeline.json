{
    "seed": 0,
    "epochs": 1,
    "max_time": "02:00:00:00",
    "input_data": {
        "dataset": "MNIST",
        "batch_size": 128,

        "pixel_representation": {
            "type": "con",
            "settings": {
                "con": {
                    "jittering": true,
                    "normalize": false
                },
                "cat": { "onehot_encoded": false }
            }
        },

        "supervised": {
            "use_labeling": false,
            "n_classes": 10
        },

        "input_shape": {
            "height": 28,
            "width": 28,
            "channels": 1
        }
    },
    "model": {
        "n_bins": 1,
        "latent_shape": {
            "height": 1,
            "width": 1,
            "channels": 16
        },
        "decoder_net": {
            "n_filters": 32,
            "batch_norm": true,
            "bias": false,
            "resblock": true,
            "out_channels": 2
        },
        "learn_std": true,
        "min_std": 1e-3,
        "max_std": 1.0,
        "K": 1,
        "n_chunks": 32,
        "missing": null,
        "layers": 4
    },
    "clustering": {
        "use_clustering": false,
        "n_clusters": 100,
        "cluster_id": 0
    }
}